{
  "devCommand": "uvicorn main:app --host 0.0.0.0 --port 3000",
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3" } 
    }
  ],
  "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ]
}
